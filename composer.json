{
    "name": "selene-framework/installers",
    "type": "composer-installer",
    "license": "MIT",
    "description": "A Composer library installer for the Selene Framework",
    "keywords": [
        "installer",
        "Selene Framework"
    ],
    "homepage": "https://github.com/selene-framework/installers",
    "authors": [
        {
            "name": "Kyle Robinson Young",
            "email": "kyle@dontkry.com",
            "homepage": "https://github.com/shama"
        },
        {
            "name": "Impactwave",
            "email": "impactwave@impactwave.com",
            "homepage": "http://impactwave.com"
        }
    ],
    "autoload": {
        "psr-0": { "Selene\\Installers\\": "src/" }
    },
    "extra": {
        "class": "Selene\\Installers\\Installer",
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "require-dev": {
        "composer/composer": "1.0.*@dev",
        "phpunit/phpunit": "4.1.*"
    }
